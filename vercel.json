{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "outputDirectory": "dist",
  "public": true,
  "functions": {
    "server/_core/index.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/trpc/(.*)",
      "dest": "/server/_core/index.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/_core/index.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "status": 200
    }
  ]
}
